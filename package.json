{
  "name": "padma-frontend",
  "version": "0.1.0",
  "private": true,
  "author": "padma.io",
  "description": "Padma.io tibetan language services",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:buildall": "env VUE_APP_API_URL=http://api.padma.io vue-cli-service electron:build -mwl --x64 --ia32",
    "electron:buildall:staging": "env VUE_APP_API_URL=http://staging-api.padma.io vue-cli-service electron:build -mwl --x64 --ia32",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "electron:generate-icons": "electron-icon-builder --input=./public/img/icons/padma.png --output=build --flatten"
  },
  "main": "background.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/vue-fontawesome": "^0.1.10",
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.15.0",
    "core-js": "^3.6.5",
    "d3": "^5.16.0",
    "electron-icon-builder": "^1.0.2",
    "register-service-worker": "^1.7.1",
    "typeface-jomolhari": "^1.1.3",
    "vue": "^2.6.11",
    "vue-context": "^5.2.0",
    "vue-router": "^3.2.0",
    "vue-toasted": "^1.1.28",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.0",
    "@vue/cli-plugin-eslint": "^4.4.0",
    "@vue/cli-plugin-pwa": "^4.4.0",
    "@vue/cli-plugin-router": "^4.4.0",
    "@vue/cli-plugin-vuex": "^4.4.0",
    "@vue/cli-service": "^4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "electron": "^9.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.4",
    "vue-template-compiler": "^2.6.11"
  }
}
